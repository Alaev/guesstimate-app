.grid
  table
    width: 100%
    table-layout: fixed
    tr
      width 100%
    td
      padding 13px 8px;
      vertical-align:top;
      .cell
        cursor pointer
        .cell-full
          width 100%
          height 100%j
        border-radius: 3px
        width: 100%;
        min-height: 90px
        border: 2px solid rgba(0,0,0,0);
        &:focus
          outline none
        &.empty:hover
          background-color: #ECF2F5;
        &.empty:focus
          border-color: #6CAED1;
